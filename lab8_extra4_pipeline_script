stage('Create') {
    node('master') { 
        sh: 'echo Running on $NODE_NAME';
        sh: 'echo "This is my message !" > file';
        stash name: "first-stage", includes: file;
    }
}
stage('Print') {
    node('slave1_jenkins-06-02') { 
        sh: 'echo Running on $NODE_NAME';
        unstash "first-stage"
        sh: 'cat file';
    }
}
